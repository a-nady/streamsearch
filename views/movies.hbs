

<style>
form {
    margin: 0 auto;
    width:250px;
}
.tftable {
    padding: 10px;
    margin-left: auto;
    margin-right: auto;
    text-align: center;
    font-size: 12px;
    color: #333333;
    width: 90%;
    border-width: 1px;
    border-collapse: collapse;
}

.tftable th {
    font-size: 12px;
    background-color: #fff;
    border-width: 1px;
    padding: 8px;
    border-style: solid;
    text-align: center;
}

.tftable tr {
    background-color: #ffffff;
}

.tftable td {
    font-size: 12px;
    border-width: 1px;
    padding: 8px;
    border-style: solid;
    border-color: #a9a9a9;
}


</style>

<form action="/dashboard/:wlist" method="post">
    <div>
        <label>Search for a Movie</label>
        <input type="text" name="query"/>
    </div>
    <div>
        <input type="submit" value="Search"/>
    </div>
</form>


<table class="tftable" border="1">
    <tr>
        <th>Title</th>
        <th>Year of Release</th>
        <th>Available at</th>
    </tr>
    {{#each list}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.release}}</td>
            <td>
            {{#each this.services}}
                <li>{{this}}</li>
            {{/each}}</td>
        </tr>
    {{/each}}

</table>
